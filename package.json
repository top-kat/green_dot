{
  "name": "green_dot",
  "version": "0.0.32",
  "main": ".",
  "types": "src/index.ts",
  "description": "A powerful TypeScript nodeJs framework for building DX friendly, secure and type-safe backend services with MongoDB integration",
  "license": "MIT",
  "author": "https://github.com/top-kat",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/top-kat/green_dot.git"
  },
  "homepage": "https://github.com/top-kat/green_dot",
  "bugs": {
    "url": "https://github.com/top-kat/green_dot/issues"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "framework",
    "mongodb",
    "api",
    "typesafe",
    "security"
  ],
  "exports": {
    ".": "./index.js",
    "./types": "./src/core-types.ts",
    "./src/*": "./dist/*.js"
  },
  "scripts": {
    "clean": "rm -rf ./dist || true && ts-node ./src/cli/cliEntryPoint.ts clean",
    "build": "tscp && tsc",
    "tsc": "tsc",
    "build:watch": "tscp & tsc -w --preserveWatchOutput",
    "test": "jest --watchAll",
    "// RELEASE": "",
    "publish": "yarn clean && tsc && yarn test:ci && npm run bump:patch",
    "bump": "yarn clean && tsc && yarn test:ci && bump --patch",
    "bump:patch": "yarn clean && tsc && yarn test:ci && bump --patch",
    "test:ci": "jest --ci --runInBand --maxConcurrency=1 --silent"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.4.0",
    "@types/node": "^22.13.13",
    "@types/typescript": "^2.0.0",
    "axios": "^1.7.4",
    "body-parser": "^1.20.2",
    "chokidar": "^4.0.3",
    "command-line-application": "^0.10.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-xml-bodyparser": "^0.3.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.3.0",
    "good-cop": "^1.3.8",
    "mongoose": "^8.12.1",
    "multer": "^1.4.5-lts.1",
    "node-schedule": "^2.1.0",
    "node-telegram-bot-api": "^0.64.0",
    "nodemailer": "^6.8.0",
    "rest-test2": "^1.0.12",
    "send-teams-message": "^1.0.1",
    "simple-cli-prompt": "^1.0.5",
    "simple-file-templater": "^1.0.5",
    "swagger-ui-express": "^5.0.1",
    "topkat-utils": "1.3.17",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.3",
    "@types/multer": "^1.4.12",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-jest": "^29.7.0",
    "bump-simple": "^1.0.29",
    "bun": "^1.2.8",
    "eslint": "^8.56.0",
    "istanbul": "^0.4.5",
    "istanbul-merge": "^2.0.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript-cp": "^0.1.9",
    "typescript-generic-types": "1.0.12"
  },
  "bin": {
    "dot": "src/cli/cliEntryPoint.ts",
    "green_dot": "src/cli/cliEntryPoint.ts",
    "gd": "src/cli/cliEntryPoint.ts"
  },
  "files": [
    "*.ts",
    "modelTypes/*.ts",
    "constants/*.ts",
    "*"
  ]
}
