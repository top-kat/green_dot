{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": [
      "astro/client"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    ".astro/types.d.ts",
    "src/**/*",
    "**/*.ts",
    "**/*.tsx",
    "**/*.mdx"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}