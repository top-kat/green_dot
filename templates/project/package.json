{
  "name": "$$projectName",
  "version": "0.0.0",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "==--->     START     <---==": "",
    "dev": "dot dev",
    "start": "dot start",
    "start:test": "NODE_ENV=test dot dev",
    "start:dev": "dot start",
    "start:prod": "NODE_ENV=production dot start",
    "start:bangk-backend-docker": "NODE_ENV=docker node ./dist/backend/app.js",
    "==--->    PUBLISH    <---==": "",
    "publishSdks": "dot publishSdks",
    "==--->     BUILD     <---==": "",
    "build": "dot build",
    "build:prod": "NODE_ENV=production dot build",
    "==--->    GENERATE   <---==": "",
    "generate": "dot generate",
    "==--->      TEST     <---==": "",
    "test:api": "dot test",
    "start:backend-ci": "concurrently -k --success=first --kill-others-on-fail \"NODE_ENV=test dot start\" \"sh -c 'sleep 5 && dot test --ci'\"",
    "// This is for starting backend in test mode": "",
    "start:test-sleep": "sh -c 'sleep 5 && dot test'",
    "coverage": "bun ./coverage.ts",
    "test": "jest --ci --runInBand --maxConcurrency=1 --no-clean",
    "==--->     CLEAN     <---==": "",
    "clean": "find . -type d -name dist -not -path '*/node_modules/*' -exec rm -rf {} \\; && find . -type d -name node_modules -prune -exec rm -rf {} \\;",
    "==--->     MISC      <---==": "",
    "countLines": "npx cloc ./ --exclude-dir node_modules",
    "==--->   SHORTCUTS   <---==": "",
    "g": "yarn generate",
    "t": "yarn test:api"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "crypto-js": "^4.2.0",
    "green_dot": "^0.0.41",
    "js-sha256": "^0.11.0",
    "jsonwebtoken": "^9.0.2",
    "topkat-utils": "^1.3.17",
    "typescript": "^5.3.3",
    "typescript-generic-types": "^1.0.12"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-jest": "^29.7.0",
    "bun": "^1.2.10",
    "concurrently": "^9.1.2",
    "eslint": "^8.56.0",
    "istanbul": "^0.4.5",
    "istanbul-merge": "^2.0.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": "22.x"
  }
}